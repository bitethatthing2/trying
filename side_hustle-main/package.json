{
  "name": "sidehustle",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https",
    "dev:https": "node server.js",
    "dev:port3000": "next dev --experimental-https --port 3000",
    "dev:trusted": "node server.js",
    "generate-certs": "node generate-certs.js",
    "setup-trusted-dev": "npm run generate-certs && npm run dev:trusted",
    "build": "npm install && next build",
    "netlify-build": "cross-env next build && node netlify-build.js",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.49.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "firebase": "^10.12.2",
    "firebase-admin": "^12.2.0",
    "fs-extra": "^11.2.0",
    "lucide-react": "^0.394.0",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.10.0",
    "@types/node": "^20.17.24",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "next": "^15.2.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
